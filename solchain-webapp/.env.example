# Environment Configuration Example for SolChain

# This file contains example environment variables for the SolChain application.
# Rename this file to .env and fill in the required values.

# Backend API URL
REACT_APP_BACKEND_API_URL=http://localhost:5000/api

# Blockchain Configuration
REACT_APP_BLOCKCHAIN_NETWORK=development
REACT_APP_BLOCKCHAIN_PROVIDER_URL=http://localhost:8545
REACT_APP_CONTRACT_ADDRESS=0xYourContractAddressHere

# Token Configuration
REACT_APP_SOLAR_TOKEN_CONTRACT=0xYourSolarTokenContractAddressHere
REACT_APP_SOLAR_TOKEN_DECIMALS=18

# Payment Gateway Configuration
REACT_APP_PAYMENT_GATEWAY_URL=https://yourpaymentgateway.com/api

# Other Configurations
REACT_APP_ENV=development
REACT_APP_DEBUG=true